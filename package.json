{
  "name": "homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev & npm run styles",
    "build": "npm i --legacy-peer-deps && npx tailwindcss -i ./pages/index.css -o ./pages/tw.css && next build --legacy-peer-deps",
    "start": "next start",
    "start:prod": "next build && NODE_ENV=production next start",
    "styles": "npx tailwindcss -i ./pages/index.css -o ./pages/tw.css --watch",
    "lint": "next lint",
    "export": "next export"
  },
  "dependencies": {
    "@radix-ui/react-navigation-menu": "^1.1.1",
    "@radix-ui/react-tabs": "^1.0.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "next": "12.3.1",
    "postcss": "^8.4.24",
    "react": "^16.14.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "react-typed": "^1.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1"
  }
}
